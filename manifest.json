{
	"manifest_version": 2,
	"name": "AudioVisualizer",
	"description": "turn on some music and turn your browser into a party browser.",
	"version": "2.0",
	"permissions": [
		"tabCapture",
		"tabs",
		"activeTab",
		"http://*/",
		"https://*/"
	],
	"browser_action": {
		"default_icon": "gfx/icon.png",
		"default_title": "AudioVisualizer"
	},
	"background": {
		"scripts": [
			"backgroundScripts/extensionFrontEnd.js",
			"backgroundScripts/main.js",
			"settings/setting.js"
		],
		"persistent": false
	}
}
