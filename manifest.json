{
  "manifest_version": 2,

  "name": "AudioVisualizer Drop-Down",
  "description": "dropDoWNVISUALIZER",
  "version": "1.0",

 "permissions": [
    "tabCapture",
    "activeTab",
	"tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "dropDownAudio"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
